    3  ls
    4  watch git pull
    5  sudo su
    6  git clone -b v2.4.x-release --depth 1 https://github.com/martinspp/bigbluebutton.git
    7  cd bigbluebutton/bigbluebutton-html5
    8  curl https://install.meteor.com/ | sh
    9  grep "wsUrl" /usr/share/meteor/bundle/programs/server/assets/app/config/settings.yml
   10  vi private/config/settings.yml
   11  sudo systemctl stop bbb-html5
   12  vi /etc/nginx/sites-enabled/bigbluebutton
   13  ls
   14  vi /etc/nginx/nginx.conf
   15  vi /etc/nginx/sites-available/default
   16  vi /etc/nginx/sites-enabled/default
   17  vi /etc/nginx/sites-enabled/bigbluebutton
   18  ls

wget -qO- https://ubuntu.bigbluebutton.org/bbb-install.sh | bash -s -- -g -v bionic-24 -s bbb.4eks1s.com -e info@4eks1s.com
cd bigbluebutton/
ls
watch git pull
sudo su
git clone -b v2.4.x-release --depth 1 https://github.com/martinspp/bigbluebutton.git
cd bigbluebutton/bigbluebutton-html5
curl https://install.meteor.com/ | sh
grep "wsUrl" /usr/share/meteor/bundle/programs/server/assets/app/config/settings.yml
vi private/config/settings.yml
sudo systemctl stop bbb-html5
sudo vi /etc/bigbluebutton/nginx/bbb-html5.nginx
sudo systemctl reload nginx
docker exec greenlight-v2 bundle exec rake user:create["admin","admin@example.com","o2ZTL4BRKjoVNb3iwn5^","admin"]
meteor npm install
npm start
